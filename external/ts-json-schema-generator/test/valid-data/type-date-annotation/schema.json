      "properties": {
        "dateOnly": {
          "format": "date",
          "type": "string"
        },
        "defaultFormat": {
          "examples": [
            "2020-01-01T00:00:00.000Z"
          ],
          "format": "date-time",
          "type": "string"
        },
        "time": {
          "examples": [
            "12:00:00"
          ],
          "format": "time",
          "type": "string"
        }
      },
      "required": [
        "defaultFormat",
        "time",
        "dateOnly"
      ],
      "type": "object"
    }
  }
}
